 RowBox[{
  RowBox[{
   RowBox[{"Fmin", "[", 
    RowBox[{"A_", ",", "m_"}], "]"}], ":=", " ", 
   FractionBox[
    RowBox[{
     RowBox[{"(", 
      RowBox[{"2", "-", 
       SuperscriptBox[
